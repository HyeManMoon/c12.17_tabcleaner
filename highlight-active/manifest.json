{
	"manifest_version": 2,

	"name": "Highlighting tabs",
	"description": "This extension will highlight specific tabs",
	"version": "1.0",
	"background": {
		"persistent": false,
		"scripts": ["./auto.js"]
	},

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": ["tabs"]
}
