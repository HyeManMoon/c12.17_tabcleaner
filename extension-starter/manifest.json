{
	"name": "My Extension",
	"manifest_version": 2,
	"version": "1.0",
	"description": "Senior Project Tab Cleaner Test Environment.",
	"background": {
		"persistent": false,
		"scripts": ["popup.js"]
	},
	"permissions": ["tabs"],
	"browser_action": {
		"default_title": "Test Environment",
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	}
}
